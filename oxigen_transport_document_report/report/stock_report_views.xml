<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_report_transport_document" model="ir.actions.report">
        <field name="name">Transport Document</field>
        <field name="model">stock.picking</field>
        <field name="report_type">qweb-pdf</field>
        <field
            name="report_name"
        >oxigen_transport_document_report.report_transport_document</field>
        <field
            name="report_file"
        >oxigen_transport_document_report.report_transport_document</field>
        <field
            name="print_report_name"
        >'Transport Document - %s - %s' % (object.partner_id.name or '', object.name)</field>
        <field name="binding_model_id" ref="stock.model_stock_picking" />
        <field name="binding_type">report</field>
    </record>
</odoo>
